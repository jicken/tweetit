             xmlns:e="urn:import:com.google.gwt.editor.ui.client"
             xmlns:mod="urn:import:de.adorsys.tweetitgui.widgets"
             xmlns:following="urn:import:de.adorsys.tweetitgui.following"
      >
   <ui:with field='msg' type='de.adorsys.tweetitgui.Messages'/>
